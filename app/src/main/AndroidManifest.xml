<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.wjustudio.phoneManager"
          xmlns:android="http://schemas.android.com/apk/res/android"
          android:versionCode="1"
          android:versionName="天天开心版">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".Common.PhoneManagerApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.wjustudio.phoneManager.activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.WelcomeGuideActivity"/>
        <activity android:name=".activities.HomeActivity"/>
        <activity android:name=".activities.LoginActivity"/>
        <activity android:name=".activities.SecurityActivity"/>
        <activity android:name=".activities.SecuritySettingActivity"/>
    </application>

</manifest>
