<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DiscView">
        <!--圆圈线的宽度-->
        <attr name="dvStrokenWidth" format="dimension"/>
        <!--圆圈颜色-->
        <attr name="dvStrokenColor" format="color"/>
        <!--外装饰圆是否显示-->
        <attr name="dvOuterCircleShow" format="boolean"/>
        <!--外装饰圆距离圆环边距-->
        <attr name="dvOuterCirclePad" format="dimension"/>

        <!--内部装饰圆是否显示-->
        <attr name="dvInnerCircleShow" format="boolean"/>
        <!--内部装饰圆距离圆环边距-->
        <attr name="dvInnerCirclePad" format="dimension"/>

        <!--底部圆形是否显示-->
        <attr name="dvBottomCircleIsShow" format="boolean"/>

        <!--圆形指示图片-->
        <attr name="dvIndicatorDraw" format="reference"/>

        <!--圆盘开始旋转角度值-->
        <attr name="dvStartRotateAngle" format="integer"/>
        <!--圆盘可旋转角度范围-->
        <attr name="dvAngleRotateSpan" format="integer"/>

        <!--圆盘模式-->
        <attr name="dvCircleMode">
            <enum name="normal" value="1"/>
            <enum name="round" value="2"/>
            <enum name="dot" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension"/>
        <attr name="border_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="SwitchButton">
        <attr name="kswThumbDrawable" format="reference"/>
        <attr name="kswThumbColor" format="color|reference"/>
        <attr name="kswThumbMargin" format="dimension|reference"/>
        <attr name="kswThumbMarginTop" format="dimension|reference"/>
        <attr name="kswThumbMarginBottom" format="dimension|reference"/>
        <attr name="kswThumbMarginLeft" format="dimension|reference"/>
        <attr name="kswThumbMarginRight" format="dimension|reference"/>
        <attr name="kswThumbWidth" format="dimension|reference"/>
        <attr name="kswThumbHeight" format="dimension|reference"/>
        <attr name="kswThumbRadius" format="dimension|reference"/>
        <attr name="kswBackRadius" format="dimension|reference"/>
        <attr name="kswBackDrawable" format="reference"/>
        <attr name="kswBackColor" format="color|reference"/>
        <attr name="kswFadeBack" format="boolean"/>
        <attr name="kswBackMeasureRatio" format="float"/>
        <attr name="kswAnimationDuration" format="integer"/>
        <attr name="kswTintColor" format="color|reference"/>
        <attr name="kswTextOn" format="string"/>
        <attr name="kswTextOff" format="string"/>
        <attr name="kswTextMarginH" format="dimension"/>
    </declare-styleable>


    <string-array name="icon_name">
        <item>手机防盗</item>
        <item>通讯卫士</item>
        <item>进程管理</item>
        <item>软件管家</item>
        <item>缓存清理</item>
    </string-array>
    <string-array name="proof_title">
        <item>换卡通知</item>
        <item>定位手机位置</item>
        <item>远程锁屏手机</item>
        <item>远程清理数据</item>
        <item>播放报警音乐</item>
    </string-array>

    <string-array name="proof_info">
        <item>SIM卡更换时，锁屏并向安全号码发送短信</item>
        <item>回复#L防盗密码可定位手机位置</item>
        <item>回复#S防盗密码可定远程锁屏手机</item>
        <item>回复#P防盗密码远程清除手机的数据</item>
        <item>回复#W防盗密码可播放报警音乐</item>

    </string-array>

    <string-array name="left_menu_text">
        <item>二维码扫描</item>
        <item>通讯录备份</item>
        <item>归属地查询</item>
    </string-array>
</resources>